# Define an Arrowhead
<http://example.org/arrowhead/1>
  a crm:E24_Physical_Man-Made_Thing;
  ah:hasId "AH001";
  ah:shape ah-shape:triangle;
  ah:variant ah-variant:flat;
  ah:point true;
  ah:body true;
  ah:base ah-base:straight;
  ah:mode ah-chippingMode:plane;
  ah:amplitude true;
  ah:direction ah-chippingDirection:direct;
  ah:orientation true;
  ah:delineation ah-chippingDelineation:continuous;
  ah:chippinglocation-Side ah-chippingLocation:distal;
  ah:chippinglocation-Transversal ah-chippingLocation:proximal;
  ah:chippingShape ah-chippingShape:straight.

# Define an Excavation
<http://example.org/excavation/1>
  a crmarchaeo:A9_Archaeological_Excavation;
  dct:identifier "EX001";
  dul:hasLocation <http://example.org/place/1>;
  excav:hasPersonInCharge <http://example.org/archaeologist/1>;
  excav:hasContext <http://example.org/context/1>.

# Define a Context (Processing Unit)
<http://example.org/context/1>
  a crmarchaeo:A1_Excavation_Processing_Unit;
  dct:identifier "CTX001";
  excav:hasSVU <http://example.org/svu/1>.

# Define a Stratigraphic Volume Unit (SVU)
<http://example.org/svu/1>
  a crmarchaeo:A2_Stratigraphic_Volume_Unit;
  dct:identifier "SVU001";
  dct:description "A stratigraphic unit containing arrowheads.";
  excav:hasTimeline <http://example.org/timeline/1>.

# Define a Timeline
<http://example.org/timeline/1>
  a time:TemporalEntity;
  time:hasBeginning <http://example.org/instant/1>;
  time:hasEnd <http://example.org/instant/2>.

# Define Time Instants
<http://example.org/instant/1>
  a time:Instant;
  time:inXSDYear "1500"^^xsd:gYear;
  excav:bc true.

<http://example.org/instant/2>
  a time:Instant;
  time:inXSDYear "1400"^^xsd:gYear;
  excav:bc true.

# Define an Archaeologist
<http://example.org/archaeologist/1>
  a excav:Archaeologist;
  foaf:account <http://example.org/archaeologist/1/account>;
  foaf:name "Dr. John Doe";
  foaf:mbox <mailto:johndoe@example.org>.

# Define a Place (Location of Excavation)
<http://example.org/place/1>
  a dbo:Place;
  dbo:informationName "Excavation Site A";
  excav:hasGPSCoordinates <http://example.org/coordinates/1>.

# Define GPS Coordinates
<http://example.org/coordinates/1>
  a geo:SpatialThing;
  geo:lat "40.7128"^^xsd:decimal;
  geo:long "-74.0060"^^xsd:decimal.

# Define an Encounter Event
<http://example.org/encounter/1>
  a crmsci:S19_Encounter_Event;
  dct:date "2023-10-01"^^xsd:date;
  dbo:depth "1.5"^^xsd:decimal;
  excav:foundInSVU <http://example.org/svu/1>;
  excav:foundInAContext <http://example.org/context/1>;
  excav:foundInAExcavation <http://example.org/excavation/1>.

# Link the Arrowhead to the Encounter Event
<http://example.org/arrowhead/1>
  excav:foundInAExcavation <http://example.org/excavation/1>;
  excav:foundInAContext <http://example.org/context/1>;
  excav:foundInSVU <http://example.org/svu/1>.